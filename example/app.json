{
  "expo": {
    "name": "blueswarm-example",
    "slug": "blueswarm-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "../app.plugin.js",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 33
          },
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.tomasrav.blueswarm.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "me.tomasrav.blueswarm.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
